    "event_values = ['lead', 'conversion','staticpage','checkout','search engine hit', 'generic listing', 'ad campaign hit', 'visited site', 'brand listing', 'viewed product']\n",
    "def findBrand(x):\n",
    "    if(pd.isna(x)):\n",
    "        return np.nan\n",
    "    for key,value in brands.items():\n",
    "        if(key in x.lower()):\n",
    "            return value\n",
    "    return np.nan\n",
    "    "
